#: help.c:419
#, c-format
msgid "  SINGLESTEP\n"
"    single-step mode (same as -s option)\n"
msgstr "  SINGLESTEP\n"
"    покроковий режим (як з параметром -s)\n"

#: help.c:421
#, c-format
msgid "  SQLSTATE\n"
"    SQLSTATE of last query, or \"00000\" if no error\n"
msgstr "  SQLSTATE\n"
"    SQLSTATE останнього запиту, або \"00000\" якщо немає помилок\n"

#: help.c:423
#, c-format
msgid "  USER\n"
